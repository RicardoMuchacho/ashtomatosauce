{
  "expo": {
    "name": "Ash Tomato Sauce",
    "slug": "native_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mangaLogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rickalon.native_app", "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/mangaLogoIcon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/mangaLogo.png"
    }
  }
}
